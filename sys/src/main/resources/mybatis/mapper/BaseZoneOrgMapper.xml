<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bp.sys.mapper.BaseZoneOrgMapper">

    <!-- 带参数查询分区组织列表 -->
    <select id="selectList" resultType="java.util.Map">
        select
        uuid,parent_id,zone_org_name,zone,inserter,insert_time,updater,update_time
        from base_zone_org where 1=1
    </select>
</mapper>

